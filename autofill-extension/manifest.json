{
  "manifest_version": 2,
  "name": "AFM Autofill",
  "version": "2.0",
  "description": "Automatically fills the AFM form",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js","jquery-validate.min.js", "content.js"]
    }
  ]
}